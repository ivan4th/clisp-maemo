	$(COMMENT5) ansidecl.d ansidecl.c
	$(RM) ansidecl.d
	$(CC) $(CFLAGS) $(CLFLAGS) -eansidecl.exe ansidecl.c
	$(RM) ansidecl.c

varbrace.exe : ..\utils\varbrace.d comment5.exe ansidecl.exe
