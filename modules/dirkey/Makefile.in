# Makefile for CLISP module set dirkey

CC = @CC@
CPPFLAGS = @CPPFLAGS@
CFLAGS = @CFLAGS@
CLISP = clisp -q -norc
INCLUDES = $$($(CLISP) -b)/linkkit

LN = ln

MAKE = make

SHELL = /bin/sh

DISTRIBFILES = dirkey.o link.sh Makefile dirkey1.lisp preload.lisp
distribdir =

all : dirkey.o dirkey1.fas

dirkey1.fas : dirkey1.lisp
	$(CLISP) -c dirkey1.lisp

dirkey.m.c: dirkey.c
	$(CLISP) -C $(INCLUDES)/modprep dirkey.c

dirkey.o : dirkey.m.c config.h
	$(CC) $(CPPFLAGS) $(CFLAGS) -I$(INCLUDES) -c dirkey.m.c -o dirkey.o

# Make a module
clisp-module : all

# Make a module distribution into $(distribdir)
clisp-module-distrib : clisp-module force
	$(LN) $(DISTRIBFILES) $(distribdir)

clean : force
	rm -f core *.o *.a *.m.c *.fas *.lib

distclean : clean

force :

