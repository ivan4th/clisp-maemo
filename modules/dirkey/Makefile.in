# Makefile for CLISP module set wildcard

CC = @CC@
CFLAGS = @CFLAGS@
INCLUDES= ../
MODPREP = ../clisp -q -norc ../modprep
CLISP =

LN = ln

MAKE = make

SHELL = /bin/sh

DISTRIBFILES = dirkey.o link.sh Makefile dirkey1.lisp preload.lisp
distribdir =

all : dirkey.o dirkey1.fas

dirkey1.fas : dirkey1.lisp
	$(CLISP) -c dirkey1.lisp

dirkey.m.c: dirkey.c
	$(MODPREP) dirkey.c

dirkey.o : dirkey.m.c
	$(CC) $(CFLAGS) -I$(INCLUDES) -c dirkey.m.c -o dirkey.o

# Make a module
clisp-module : all

# Make a module distribution into $(distribdir)
clisp-module-distrib : clisp-module force
	$(LN) $(DISTRIBFILES) $(distribdir)

clean : force
	rm -f core *.o *.a *.m.c *.fas *.lib

distclean : clean

force :

