file_list=''
mod_list=''
if test -r regexp.c; then
  file_list="$file_list"' regexp.o'
  mod_list="$mod_list"' regexp'
fi
if test -z "@REGEX_O@"; then
  rm -f regex.*; # use system-wide regex implementation
fi
make clisp-module CC="${CC}" CFLAGS="${CFLAGS}" INCLUDES="$absolute_linkkitdir"
NEW_FILES="$file_list regexi.o @REGEX_O@ regexp.dvi"
NEW_LIBS="$file_list regexi.o @REGEX_O@"
NEW_MODULES="$mod_list"
TO_LOAD='regexp'
