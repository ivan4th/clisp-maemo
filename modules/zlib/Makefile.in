# Makefile for CLISP module set zlib

CC = @CC@
CPPFLAGS = @CPPFLAGS@
CFLAGS = @CFLAGS@
INCLUDES= ..

CLISP = clisp -q -norc

LN = ln

MAKE = make

SHELL = /bin/sh

DISTRIBFILES = zlib.o link.sh Makefile zlib.lisp
distribdir =

all : zlib.o zlib.fas

zlib.c zlib.fas : zlib.lisp
	$(CLISP) -c zlib.lisp

zlib.o : zlib.c $(INCLUDES)/clisp.h config.h
	$(CC) $(CPPFLAGS) $(CFLAGS) -I$(INCLUDES) -c zlib.c

config.status : configure
	sh config.status --recheck

config.h : config.status config.h.in
	sh config.status --header=config.h

Makefile : Makefile.in config.status
	sh config.status --file=Makefile

# Make a module
clisp-module : all

# Make a module distribution into $(distribdir)
clisp-module-distrib : clisp-module force
	$(LN) $(DISTRIBFILES) $(distribdir)

clean : force
	rm -f core *.o *.a

distclean : clean

force :

