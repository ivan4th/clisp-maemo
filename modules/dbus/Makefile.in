# Makefile for CLISP module set dbus

CC = @CC@
CPPFLAGS = @CPPFLAGS@
CFLAGS = @CFLAGS@
# defined by PKG_CHECK_MODULES
DBUS_CFLAGS = @DBUS_CFLAGS@
DBUS_LIBS = @DBUS_LIBS@

CLISP = clisp -q -norc
INCLUDES = $$($(CLISP) -b)/linkkit

LN = ln

MAKE = make

SHELL = /bin/sh

DISTRIBFILES = dbus.o link.sh Makefile dbus.lisp
distribdir =

all : dbus.o dbus.fas

dbus.c dbus.fas : dbus.lisp
	$(CLISP) -c dbus.lisp

dbus.o : dbus.c config.h
	$(CC) $(CPPFLAGS) $(CFLAGS) -I$(INCLUDES) $(DBUS_CFLAGS) -c dbus.c

# Make a module
clisp-module : all

# Make a module distribution into $(distribdir)
clisp-module-distrib : clisp-module force
	$(LN) $(DISTRIBFILES) $(distribdir)

clean : force
	rm -f core *.o *.a

distclean : clean

force :

